<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="page.main.title" destination="product.info.main" after="-"/>
        <move element="product.info.stock.sku" destination="product.info.price" after="product.price.final"/>
        <move element="product.info.review" destination="product.info.main" before="product.info.price"/>
        <referenceContainer name="content">
            <block class="Natxo\Miprimermodulo\Block\Miprimermodulo" name="mitemplate" template="mitemplate.phtml" after="-">
       			<arguments>
            		<argument name="cat_id" xsi:type="string">14</argument>
       			</arguments>     	
            </block>
        </referenceContainer>
    </body>
</page>