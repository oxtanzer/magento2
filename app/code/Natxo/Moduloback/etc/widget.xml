<?xml version="1.0" ?>
<widgets xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Widget:etc/widget.xsd">
	<widget class="Natxo\Moduloback\Block\Widget\SliderImages" id="natxo_moduloback_sliderimages">
		<label>sliderImages</label>
		<description>sliderImages</description>
		<parameters>
            <parameter name="title" sort_order="1" xsi:type="text" required="false" visible="true">
                <label>Title</label>
            </parameter>
            <parameter name="message" sort_order="2" xsi:type="text" required="true" visible="true">
                <label>Message</label>
            </parameter>
			<parameter name="autoplay" sort_order="3" visible="true" xsi:type="select">
				<label>Autoplay</label>
                <options>
                    <option name="autoplay_yes" value="1" selected="true">
                        <label>YES</label>
                    </option>
                    <option name="autoplay_no" value="0">
                        <label>NO</label>
                    </option>
                </options>
			</parameter>
		</parameters>
	</widget>
</widgets>